{
  "name": "diory-react-components",
  "version": "1.2.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js",
    "lint": "eslint src --fix",
    "test": "npm run lint && npm run build && jest --env=jsdom --watch",
    "example": "npm run lint && npm run build && browserify -t [ babelify ] example/app.js -o example/bundle.js",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "prop-types": "15.5.8"
  },
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.16.0",
    "babel-register": "6.16.3",
    "babelify": "^7.3.0",
    "enzyme": "2.8.2",
    "eslint": "3.6.1",
    "eslint-config-standard": "6.2.0",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "2.0.1",
    "jest": "16.0.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-standard",
      "eslint-config-standard-react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-curly-spacing": [
        2,
        "always",
        {
          "spacing": {
            "objectLiterals": "never"
          }
        }
      ],
      "operator-linebreak": [
        "error",
        "after"
      ]
    }
  },
  "description": "Create React components from Diograph data",
  "homepage": "https://github.com/DioryMe/diory-react-components",
  "keywords": [
    "diory",
    "react",
    "javascript",
    "diograph"
  ],
  "author": "Olli-Pekka Pohjola <oopee@iki.fi>",
  "license": "MIT"
}
